                            android:orientation="vertical"
                            android:textColor="@android:color/black">


                            <TextView
                                android:id="@+id/tv_pin_code"
                                style="@style/StyleTheme"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/margin10"
                                android:layout_marginBottom="3dp"
                                android:layout_weight="1"
                                android:background="@android:color/transparent"
                                android:fontFamily="@font/robotoregular"
                                android:text="0"
                                android:textColor="@color/colorBlack"
                                android:textSize="13dp" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="@dimen/_15dp"
                        android:orientation="horizontal"

                        android:weightSum="2">

                        <TextView
                            android:id="@+id/tv_emergency_contact"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/margin10"
                            android:layout_marginBottom="3dp"
                            android:layout_weight="1"
                            android:fontFamily="@font/robotoregular"
                            android:text="@string/emergency_contact_number"
                            android:textColor="@color/colorBlack"
                            android:textSize="@dimen/text_14" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:textColor="@android:color/black">


                            <TextView
                                android:id="@+id/tv_emergency_contact_no"
                                style="@style/StyleTheme"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/margin10"
                                android:layout_marginBottom="3dp"
                                android:layout_weight="1"
                                android:background="@android:color/transparent"
                                android:fontFamily="@font/robotoregular"
                                android:text="0"
                                android:textColor="@color/colorBlack"
                                android:textSize="13dp" />

                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:orientation="vertical">

                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_15dp"
                    android:layout_marginBottom="@dimen/_3dp"
                    android:background="@drawable/shape_of_textview"
                    android:fontFamily="@font/robotoregular"
                    android:gravity="center"
                    android:padding="@dimen/_3dp"
                    android:text="@string/click_on_button_for_view_medical_information_details"
                    android:textColor="@color/menuTextColor"
                    android:textSize="@dimen/text_16"
                    android:textStyle="bold" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1dp"
                    android:background="@color/colorBlack" />
                <!--end of general details-->

            </LinearLayout>

            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scrollbars="none">

                <LinearLayout
                    android:id="@+id/ll_for_dynamic_add"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"></LinearLayout>

            </ScrollView>

        </LinearLayout>

    </ScrollView>

</LinearLayout>

